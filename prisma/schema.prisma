generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EtherFi {
  block_number    BigInt
  log_index       BigInt
  block_timestamp BigInt
  eth             String

  @@unique([block_number, log_index])
}

model RocketPool {
  block_number    BigInt @unique
  eth             String
  rpl             String
  block_timestamp BigInt
}

model StakeWise {
  block_number    BigInt
  log_index       BigInt
  vault           String
  eth             String
  block_timestamp BigInt

  @@unique([block_number, log_index])
}
